{
  "name": "angular-crudder",
  "version": "0.0.11",
  "description": "A schematic project which generates angular CRUD code based on the valid JSON file provided.",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "schematic": "schematics .:angular-crudder --name=customer --schemaPath=H:/schema.json --module=customer.module --skipModuleImport=false ",
    "schematic-build": "schematics .:ionic-crudder --name=leadSource --dry-run=false --schemaPath=H:/lead.json --changeDetection=false --apiUrl=api/leadSources --pluralName=leadSources",
    "delete-existing": ""
  },
  "keywords": [
    "schematics"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mistrykaran91/angular-crudder.git"
  },
  "author": "Karan Mistry",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^7.2.3",
    "@angular-devkit/schematics": "^7.2.3",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^8.0.31",
    "jasmine": "^2.8.0",
    "jsdom": "^15.1.1",
    "prettier": "^1.18.2",
    "typescript": "^2.5.2"
  }
}
